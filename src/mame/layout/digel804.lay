		</element>
		<element name="lc" ref="lc" state="0">
			<bounds x="72" y="24" width="50" height="5"/>
		</element>
		<element name="ld" ref="ld" state="0">
			<bounds x="72" y="31" width="50" height="5"/>
		</element>
		<element name="le" ref="le" state="0">
			<bounds x="72" y="41" width="50" height="5"/>
		</element>
		<element name="lf" ref="lf" state="0">
			<bounds x="72" y="48" width="50" height="5"/>
		</element>
		<element name="l0" ref="l0" state="0">
			<bounds x="72" y="68" width="50" height="5"/>
		</element>

		<!-- function LEDs -->
		<element name="func_led1" ref="led" state="0">
			<bounds x="10" y="10" width="5" height="5"/>
		</element>
		<element name="func_led2" ref="led" state="0">
			<bounds x="10" y="17" width="5" height="5"/>
		</element>
		<element name="func_led3" ref="led" state="0">
			<bounds x="10" y="24" width="5" height="5"/>
		</element>
		<element name="func_led4" ref="led" state="0">
			<bounds x="10" y="34" width="5" height="5"/>
		</element>
