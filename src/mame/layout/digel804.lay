		</element>

		<!-- VFD -->
		<repeat count="16">
			<param name="n" start="0" increment="1" />
			<param name="x" start="120" increment="9" />
			<element name="vfd~n~" ref="vfd0" state="0">
				<bounds x="~x~" y="25" width="9" height="17"/>
			</element>
		</repeat>

		<!-- Status LEDs -->
		<element name="input_led" ref="led" state="0">
			<bounds x="125" y="65" width="5" height="5"/>
		</element>
		<element name="input" ref="input">
			<bounds x="132" y="65" width="10" height="5" />
		</element>
		<element name="busy_led" ref="led" state="0">
			<bounds x="175" y="65" width="5" height="5"/>
		</element>
		<element name="busy" ref="busy">
			<bounds x="182" y="65" width="10" height="5" />
		</element>
		<element name="error_led" ref="led" state="0">
			<bounds x="225" y="65" width="5" height="5"/>
		</element>
		<element name="error" ref="error">
			<bounds x="232" y="65" width="10" height="5" />
		</element>
	</view>

</mamelayout>
