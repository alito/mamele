		</element>
		<element name="func_led11" ref="led" state="0">
			<bounds x="65" y="17" width="5" height="5"/>
		</element>
		<element name="func_led12" ref="led" state="0">
			<bounds x="65" y="24" width="5" height="5"/>
		</element>
		<element name="func_led13" ref="led" state="0">
			<bounds x="65" y="31" width="5" height="5"/>
		</element>
		<element name="func_led14" ref="led" state="0">
			<bounds x="65" y="41" width="5" height="5"/>
		</element>
		<element name="func_led15" ref="led" state="0">
