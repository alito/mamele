			<bounds x="224" y="359" width="7" height="7" />
		</element>
		<element name="led_ack" ref="red_led">
			<bounds x="225" y="360" width="5" height="5" />
		</element>
		<element name="label_ack" ref="label_ack">
			<bounds x="210" y="371" width="36" height="9" />
		</element>

